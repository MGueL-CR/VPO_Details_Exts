{
    "name": "VPO Details",
    "description": "Improves Work Order display, organizes data and adds a barcode/QR code to read the VPO name. Developed by Miguel Mej√≠as A (http://www.linkedin.com/in/mguel-dev).",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/src/documento(16px).png",
            "32": "/src/documento(32px).png",
            "64": "/src/documento(64px).png",
            "128": "/src/documento(128px).png"
        }},
    "icons": {
        "16": "/src/documento(16px).png",
        "32": "/src/documento(32px).png",
        "48": "/src/documento(64px).png",
        "128": "/src/documento(128px).png"
    },
    "version": "1.02",
    "manifest_version": 3,
    "content_scripts": [
        {
          "js": ["/js/libs/qrcode.min.js", "/js/libs/JsBarcode.all.min.js", "/js/components.js", "/js/app.js"],
          "css": ["/css/styles.css"],
          "matches": [
            "file:///C:/Users/*",
            "file:///home/*"
          ]
        }
      ],
    "permissions": ["fileSystem"]
}